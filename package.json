{
  "name": "molecule-alpha",
  "version": "1.0.0",
  "description": "molecule-alpha",
  "main": "index.js",
  "scripts": {
    "build": "yarn wsrun --stages -c build",
    "build:blockchain": "yarn wsrun -p nobuntu-blockchain -c build",
    "build:webapp": "yarn wsrun -p nobuntu-webapp -c build",
    "start:dev": "yarn wsrun start:dev --parallel --exclude-missing",
    "clean": "rm -rf ./node_modules && rm -rf ./*/node_modules",
    "test:webapp": "yarn wsrun -p nobuntu-webapp -c test:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/linumlabs/molecule-alpha.git"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "webapp",
      "blockchain"
    ],
    "nohoist": [
      "**/*"
    ]
  },
  "devDependencies": {
    "chalk": "^2.0.1",
    "jest": "^24.5.0",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "precommit": "^1.2.2",
    "wsrun": "^3.6.5"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.0"
  },
  "keywords": [
    "NodeJS",
    "NestJS",
    "React",
    "Redux",
    "Mongoose",
    "Monorepo",
    "Ethers",
    "Etherlime"
  ],
  "author": "Linum Labs",
  "license": "MIT",
  "homepage": "https://gitlab.com/linumlabs/molecule-alpha#readme",
  "engines": {
    "node": ">=8.0",
    "yarn": ">=1.9.4"
  },
  "dependencies": {}
}
